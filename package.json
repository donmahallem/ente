{
    "name": "ente",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "test": "mocha --timeout 10000 --check-leaks -r ts-node/register src/**/*.spec.ts",
        "start": "npm run build:live",
        "build:live": "nodemon",
        "build": "node ./node_modules/typescript/bin/tsc --outdir ./dist/ --baseUrl ./src/",
        "build:doc": "node ./node_modules/typedoc/bin/typedoc --out ./tt2/ --mode modules --exclude /**/index ./src/",
        "test-ci": "nyc mocha --opts mocha.opts",
        "build2": "webpack --config ./config/index.config.js",
        "start:dev": "webpack-dev-server --config ./config/index.config.js"
    },
    "author": "",
    "license": "MIT",
    "dependencies": {
        "moment": "^2.24.0",
        "rxjs": "^6.5.2",
        "three": "^0.105.2"
    },
    "devDependencies": {
        "@types/chai": "^4.1.7",
        "@types/chai-http": "^4.2.0",
        "@types/mocha": "^5.2.7",
        "@types/sinon": "^7.0.12",
        "@types/supertest": "^2.0.7",
        "@types/three": "^0.103.2",
        "chai": "^4.2.0",
        "chai-http": "^4.3.0",
        "clean-webpack-plugin": "^3.0.0",
        "css-loader": "^2.1.1",
        "html-webpack-plugin": "^3.2.0",
        "istanbul": "^0.4.5",
        "mocha": "^6.1.4",
        "node-sass": "^4.12.0",
        "nodemon": "^1.19.1",
        "nyc": "^14.1.1",
        "pug": "^2.0.3",
        "pug-loader": "^2.4.0",
        "sass-loader": "^7.1.0",
        "sinon": "^7.3.2",
        "sinon-chai": "^3.3.0",
        "source-map-support": "^0.5.12",
        "style-loader": "^0.23.1",
        "supertest": "^4.0.2",
        "ts-loader": "^6.0.2",
        "ts-node": "^8.2.0",
        "typedoc": "^0.14.2",
        "typescript": "^3.5.1",
        "webpack": "^4.33.0",
        "webpack-cli": "^3.3.4",
        "webpack-dev-server": "^3.7.1"
    },
    "nyc": {
        "include": [
            "src/*.ts",
            "src/**/*.ts"
        ],
        "exclude": [
            "typings",
            "src/**/*.spec.ts",
            "**/*.d.ts"
        ],
        "extension": [
            ".ts"
        ],
        "require": [
            "ts-node/register"
        ],
        "reporter": [
            "json",
            "html",
            "text-summary",
            "lcov"
        ],
        "sourceMap": true,
        "instrument": true,
        "all": true
    }
}
