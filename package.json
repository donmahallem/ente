{
    "name": "ente",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "test": "mocha --timeout 10000 --check-leaks -r ts-node/register src/**/*.spec.ts",
        "start": "npm run build:live",
        "build:live": "nodemon",
        "build": "node ./node_modules/typescript/bin/tsc --outdir ./dist/ --baseUrl ./src/",
        "build:doc": "node ./node_modules/typedoc/bin/typedoc --out ./tt2/ --mode modules --exclude /**/index ./src/",
        "test-ci": "nyc mocha --opts mocha.opts",
        "build2": "webpack --config ./config/index.config.js",
        "start:dev": "webpack-dev-server --config ./config/index.config.js",
        "lint": "tslint -c tslint.json -p tsconfig-lint.json src/**/*.ts",
        "lint:fix": "tslint --fix -c tslint.json -p tsconfig-lint.json src/**/*.ts"
    },
    "author": "",
    "license": "MIT",
    "dependencies": {
        "moment": "~2.30.1",
        "rxjs": "~7.8.2",
        "three": "~0.179.1"
    },
    "devDependencies": {
        "@types/chai": "5.2.2",
        "@types/chai-http": "4.2.4",
        "@types/mocha": "10.0.10",
        "@types/node": "22.17.1",
        "@types/sinon": "17.0.4",
        "@types/supertest": "6.0.3",
        "@types/three": "0.179.0",
        "chai": "5.2.1",
        "chai-http": "5.1.2",
        "clean-webpack-plugin": "4.0.0",
        "css-loader": "7.1.2",
        "html-webpack-plugin": "5.6.4",
        "istanbul": "0.4.5",
        "mocha": "11.7.1",
        "node-sass": "7.0.3",
        "nodemon": "3.1.10",
        "nyc": "17.1.0",
        "pug": "3.0.3",
        "pug-loader": "2.4.0",
        "sass-loader": "16.0.5",
        "sinon": "21.0.0",
        "sinon-chai": "4.0.0",
        "source-map-support": "0.5.21",
        "style-loader": "4.0.0",
        "supertest": "7.1.4",
        "ts-loader": "9.5.2",
        "ts-node": "10.9.2",
        "tslint": "6.1.3",
        "typedoc": "0.28.10",
        "typescript": "5.9.2",
        "webpack": "5.101.1",
        "webpack-cli": "6.0.1",
        "webpack-dev-server": "5.2.2"
    },
    "nyc": {
        "include": [
            "src/*.ts",
            "src/**/*.ts"
        ],
        "exclude": [
            "typings",
            "src/**/*.spec.ts",
            "**/*.d.ts"
        ],
        "extension": [
            ".ts"
        ],
        "require": [
            "ts-node/register"
        ],
        "reporter": [
            "json",
            "html",
            "text-summary",
            "lcov"
        ],
        "sourceMap": true,
        "instrument": true,
        "all": true
    }
}
