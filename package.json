{
    "name": "ente",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "test": "mocha --timeout 10000 --check-leaks -r ts-node/register src/**/*.spec.ts",
        "start": "npm run build:live",
        "build:live": "nodemon",
        "build": "node ./node_modules/typescript/bin/tsc --outdir ./dist/ --baseUrl ./src/",
        "build:doc": "node ./node_modules/typedoc/bin/typedoc --out ./tt2/ --mode modules --exclude /**/index ./src/",
        "test-ci": "nyc mocha --opts mocha.opts",
        "build2": "webpack --config ./config/index.config.js",
        "start:dev": "webpack-dev-server --config ./config/index.config.js",
        "lint": "tslint -c tslint.json -p tsconfig-lint.json src/**/*.ts",
        "lint:fix": "tslint --fix -c tslint.json -p tsconfig-lint.json src/**/*.ts"
    },
    "author": "",
    "license": "MIT",
    "dependencies": {
        "moment": "~2.29.1",
        "rxjs": "~7.3.0",
        "three": "~0.132.2"
    },
    "devDependencies": {
        "@types/chai": "4.2.21",
        "@types/chai-http": "4.2.0",
        "@types/mocha": "9.0.0",
        "@types/node": "14.17.14",
        "@types/sinon": "10.0.2",
        "@types/supertest": "2.0.11",
        "@types/three": "0.131.0",
        "chai": "4.3.4",
        "chai-http": "4.3.0",
        "clean-webpack-plugin": "3.0.0",
        "css-loader": "6.2.0",
        "html-webpack-plugin": "5.3.2",
        "istanbul": "0.4.5",
        "mocha": "9.1.1",
        "node-sass": "6.0.1",
        "nodemon": "2.0.12",
        "nyc": "15.1.0",
        "pug": "3.0.2",
        "pug-loader": "2.4.0",
        "sass-loader": "12.1.0",
        "sinon": "11.1.2",
        "sinon-chai": "3.7.0",
        "source-map-support": "0.5.19",
        "style-loader": "3.2.1",
        "supertest": "6.1.6",
        "ts-loader": "9.2.5",
        "ts-node": "10.2.1",
        "tslint": "6.1.3",
        "typedoc": "0.21.9",
        "typescript": "4.4.2",
        "webpack": "5.51.1",
        "webpack-cli": "4.8.0",
        "webpack-dev-server": "4.1.0"
    },
    "nyc": {
        "include": [
            "src/*.ts",
            "src/**/*.ts"
        ],
        "exclude": [
            "typings",
            "src/**/*.spec.ts",
            "**/*.d.ts"
        ],
        "extension": [
            ".ts"
        ],
        "require": [
            "ts-node/register"
        ],
        "reporter": [
            "json",
            "html",
            "text-summary",
            "lcov"
        ],
        "sourceMap": true,
        "instrument": true,
        "all": true
    }
}
