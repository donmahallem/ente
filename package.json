{
    "name": "ente",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "test": "mocha --timeout 10000 --check-leaks -r ts-node/register src/**/*.spec.ts",
        "start": "npm run build:live",
        "build:live": "nodemon",
        "build": "node ./node_modules/typescript/bin/tsc --outdir ./dist/ --baseUrl ./src/",
        "build:doc": "node ./node_modules/typedoc/bin/typedoc --out ./tt2/ --mode modules --exclude /**/index ./src/",
        "test-ci": "nyc mocha --opts mocha.opts",
        "build2": "webpack --config ./config/index.config.js",
        "start:dev": "webpack-dev-server --config ./config/index.config.js",
        "lint": "tslint -c tslint.json -p tsconfig-lint.json src/**/*.ts",
        "lint:fix": "tslint --fix -c tslint.json -p tsconfig-lint.json src/**/*.ts"
    },
    "author": "",
    "license": "MIT",
    "dependencies": {
        "moment": "~2.24.0",
        "rxjs": "~6.5.5",
        "three": "~0.115.0"
    },
    "devDependencies": {
        "@types/chai": "~4.2.11",
        "@types/chai-http": "~4.2.0",
        "@types/mocha": "~7.0.2",
        "@types/node": "~13.11.0",
        "@types/sinon": "~9.0.0",
        "@types/supertest": "~2.0.8",
        "@types/three": "~0.103.2",
        "chai": "~4.2.0",
        "chai-http": "~4.3.0",
        "clean-webpack-plugin": "~3.0.0",
        "css-loader": "~3.4.2",
        "html-webpack-plugin": "~4.0.4",
        "istanbul": "~0.4.5",
        "mocha": "~7.1.1",
        "node-sass": "~4.13.0",
        "nodemon": "~2.0.1",
        "nyc": "~15.0.1",
        "pug": "~2.0.4",
        "pug-loader": "~2.4.0",
        "sass-loader": "~8.0.2",
        "sinon": "~9.0.1",
        "sinon-chai": "~3.5.0",
        "source-map-support": "~0.5.16",
        "style-loader": "~1.1.3",
        "supertest": "~4.0.2",
        "ts-loader": "~6.2.2",
        "ts-node": "~8.8.2",
        "tslint": "~6.1.1",
        "typedoc": "~0.17.3",
        "typescript": "~3.8.3",
        "webpack": "~4.42.1",
        "webpack-cli": "~3.3.11",
        "webpack-dev-server": "~3.10.3"
    },
    "nyc": {
        "include": [
            "src/*.ts",
            "src/**/*.ts"
        ],
        "exclude": [
            "typings",
            "src/**/*.spec.ts",
            "**/*.d.ts"
        ],
        "extension": [
            ".ts"
        ],
        "require": [
            "ts-node/register"
        ],
        "reporter": [
            "json",
            "html",
            "text-summary",
            "lcov"
        ],
        "sourceMap": true,
        "instrument": true,
        "all": true
    }
}
